{
  "name": "adobeio-cna-starter-project",
  "version": "0.1.0",
  "description": "A starter project for building a cloud native app (CNA) on top of Adobe I/O Runtime",
  "scripts": {
    "dev": "cross-env NODE_ENV=development PORT=9080 nodemon INSPECT=$NODE_DEBUG_OPTIONS --inspect $INSPECT --watch web-src/resources --watch actions/ --watch manifest.yml ./scripts/parcel.proxy.js",
    "build:ui": "cross-env NODE_ENV=production REMOTE_ACTIONS=true node scripts/build.ui.js",
    "build:actions": "cross-env NODE_ENV=production node scripts/build.actions.js",
    "build": "npm run build:ui && npm run build:actions",
    "deploy:ui": "cross-env NODE_ENV=production node scripts/deploy.ui.js",
    "deploy:actions": "cross-env NODE_ENV=production node scripts/deploy.actions.js",
    "deploy": "npm run deploy:actions && npm run deploy:ui",
    "undeploy:ui": "cross-env NODE_ENV=production node scripts/undeploy.ui.js",
    "undeploy:actions": "cross-env NODE_ENV=production node scripts/undeploy.actions.js",
    "undeploy": "npm run undeploy:actions && npm run undeploy:ui",
    "lint": "eslint actions test web-src scripts",
    "beautify": "npm run lint -- --fix",
    "test": "jest",
    "e2e": "node scripts/build.ui.js && cross-env PORT=9080 NODE_ENV=development node scripts/express.e2e.js",
    "coverage": "nyc --reporter=html --reporter=text mocha",
    "clean": "git clean -Xdf --exclude='!.env*'"
  },
  "keywords": [
    "ecosystem:adobe-io-cna",
    "aio-cna:template"
  ],
  "author": "Adobe Inc",
  "license": "Apache-2.0",
  "bugs": "https://github.com/adobe/adobeio-cna-starter-project/issues",
  "repository": "adobe/adobeio-cna-starter-project",
  "homepage": "https://github.com/adobe/adobeio-cna-starter-project",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "archiver": "^3.0.0",
    "aws-sdk": "^2.406.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "cross-env": "^5.2.0",
    "cross-spawn": "^6.0.5",
    "dotenv": "^6.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-adapter-react-helper": "^1.3.1",
    "eslint": "^5.12.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "express": "^4.16.4",
    "fs-extra": "^7.0.1",
    "http-proxy-middleware": "^0.19.0",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "js-yaml": "^3.13.1",
    "mime-types": "^2.1.22",
    "nodemon": "^1.18.6",
    "open": "^6.2.0",
    "openwhisk": "^3.18.0",
    "parcel-bundler": "^1.10.3",
    "postcss-modules": "^1.4.1",
    "prop-types": "^15.6.2",
    "pubsub-js": "^1.7.0",
    "query-string": "^6.2.0",
    "react": "^16.7.0",
    "react-dom": "^16.6.3",
    "react-error-boundary": "^1.2.3",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.8.6",
    "request-promise": "^4.2.4",
    "testcafe": "^1.1.1"
  },
  "jest": {
    "roots": [
      "<rootDir>/test/actions",
      "<rootDir>/test/ui"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "actions/**/*.js",
      "web-src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/jest.setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/helpers/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/helpers/fileMock.js"
    }
  }
}
